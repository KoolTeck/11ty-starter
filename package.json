{
  "name": "static-start",
  "description": "A starter template for building wonderful static sites.",
  "author": "Matt Waler",
  "main": "11ty.config.js",
  "license": "UNLICENSED",
  "homepage": "https://github.com/mattwaler/static-starter",
  "repository": "https://github.com/mattwaler/static-starter",
  "bugs": "https://github.com/mattwaler/static-starter/issues",
  "config": {
    "11ty": "--quiet --config=11ty.config.js"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production npm-run-all clean -p build:*",
    "build:11ty": "eleventy $npm_package_config_11ty",
    "build:rollup": "rollup -c",
    "clean": "del-cli dist",
    "dev": "npm-run-all clean build:* -p dev:*",
    "dev:11ty": "eleventy --serve $npm_package_config_11ty",
    "dev:rollup": "rollup -c -w",
    "dev:server": "json-server --watch db.json"
  },
  "browserslist": "> 2%",
  "devDependencies": {
    "@11ty/eleventy": "^0.9.0",
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@rollup/plugin-replace": "^2.3.1",
    "@tailwindcss/custom-forms": "^0.2.1",
    "autoprefixer": "^9.6.2",
    "cross-env": "^6.0.3",
    "del-cli": "^3.0.0",
    "json-server": "^0.15.1",
    "npm-run-all": "^4.1.5",
    "postcss-import": "^12.0.1",
    "prettier": "^1.19.1",
    "rollup": "^1.29.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-svelte": "^5.1.1",
    "rollup-plugin-terser": "^5.1.3",
    "svelte": "^3.16.7",
    "tailwindcss": "^1.1.2"
  },
  "dependencies": {
    "alpinejs": "^1.10.0",
    "axios": "^0.19.2"
  }
}
