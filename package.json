{
  "name": "webpack-proof",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "yarn clean && NODE_ENV=production concurrently 'yarn:build:*' --raw",
    "build:assets": "webpack",
    "build:site": "eleventy --quiet --config=11ty.config.js",
    "clean": "rm -rf dist",
    "dev": "yarn clean && concurrently 'yarn:dev:*' --raw",
    "dev:assets": "webpack",
    "dev:site": "eleventy --serve --quiet --config=11ty.config.js",
    "dev:webhook": "node webhook",
    "lint": "eslint --ext .js scripts/ --fix"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.9.0",
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "autoprefixer": "^9.6.2",
    "axios": "^0.19.0",
    "babel-loader": "^8.0.6",
    "concurrently": "^4.1.2",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.5.1",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "koa": "^2.8.2",
    "koa-router": "^7.4.0",
    "mini-css-extract-plugin": "^0.8.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-loader": "^3.0.0",
    "tailwindcss": "^1.1.2",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9"
  }
}
