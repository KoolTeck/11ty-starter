{
  "name": "site-start",
  "description": "Site start.",
  "author": "Matt Waler",
  "main": "11ty.config.js",
  "license": "UNLICENSED",
  "homepage": "https://github.com/mattwaler/11ty-starter",
  "repository": "https://github.com/mattwaler/11ty-starter",
  "bugs": "https://github.com/mattwaler/11ty-starter/issues",
  "scripts": {
    "build": "cross-env NODE_ENV=production run-p build:*",
    "build:assets": "webpack",
    "build:site": "eleventy --quiet --config=11ty.config.js",
    "clean": "shx rm -rf dist",
    "dev": "run-p dev:*",
    "dev:assets": "webpack --watch",
    "dev:site": "eleventy --serve --quiet --config=11ty.config.js",
    "prebuild": "run-p clean",
    "predev": "run-s clean build:assets"
  },
  "browserslist": [
    "> 2%",
    "ie 11"
  ],
  "devDependencies": {
    "@11ty/eleventy": "^0.9.0",
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "@tailwindcss/custom-forms": "^0.2.1",
    "autoprefixer": "^9.6.2",
    "axios": "^0.19.0",
    "babel-loader": "^8.0.6",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
    "cssuseragent": "^2.1.31",
    "dotenv": "^8.2.0",
    "eslint": "^6.5.1",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "html-minifier": "^4.0.0",
    "import-glob": "^1.5.0",
    "mini-css-extract-plugin": "^0.8.0",
    "npm-run-all": "^4.1.5",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.1.2",
    "prismjs": "^1.17.1",
    "shx": "^0.3.2",
    "svelte": "^3.15.0",
    "svelte-loader": "^2.13.6",
    "tailwindcss": "^1.1.2",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9"
  }
}
